{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}